{
  "name": "Momentum",
  "productName": "Momentum",
  "version": "0.1.0",
  "description": "Esports Virtual Personal Trainer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . Momentum --overwrite --platform=darwin --arch=x64 --icon=img/8ball.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Momentum --overwrite --platform=win32 --arch=x64 --icon=img/8ball.ico --prune=true --out=release-builds --version-string.CompanyName=AW --version-string.FileDescription=AW --version-string.ProductName=\"Momentum\"",
    "create-winstaller": "node installers/create_winstaller.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/jigawhat/momentum.git"
  },
  "keywords": [
    "esports",
    "ai",
    "personal trainer"
  ],
  "author": "University College London",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/jigawhat/momentum/issues"
  },
  "homepage": "https://gitlab.com/jigawhat/momentum#README",
  "devDependencies": {
    "electron": "^5.0.8",
    "electron-packager": "^14.0.3",
    "electron-winstaller": "^4.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
